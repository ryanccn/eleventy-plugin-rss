{
  "name": "@ryanccn/eleventy-plugin-rss",
  "version": "1.2.2",
  "description": "A pack of Eleventy plugins for generating an RSS feed using the Nunjucks templating engine.",
  "publishConfig": {
    "access": "public"
  },
  "main": ".eleventy.js",
  "files": [
    "LICENSE",
    "COPYING.md",
    ".eleventy.js",
    "src"
  ],
  "scripts": {
    "sample": "cd sample && npx @11ty/eleventy --config=config-sample.js",
    "clean": "rm -rf sample/_site"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanccn/eleventy-plugin-rss.git"
  },
  "keywords": [
    "eleventy",
    "eleventy-plugin"
  ],
  "author": {
    "name": "Ryan Cao",
    "url": "https://ryanccn.dev/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanccn/eleventy-plugin-rss/issues"
  },
  "homepage": "https://github.com/ryanccn/eleventy-plugin-rss",
  "11ty": {
    "compatibility": ">=0.11"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "posthtml": "^0.16.6",
    "posthtml-urls": "1.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1"
  }
}
